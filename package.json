{
  "name": "personal",
  "version": "1.0.0",
  "description": "personal website",
  "author": "kshreve@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "personal"
  },
  "engines": {
    "node": "5.6.0"
  },
  "scripts": {
    "postinstall": "npm run heroku",
    "start-message": "babel-node tools/startMessage.js",
    "prestartDev": "npm-run-all --parallel start-message",
    "startDev": "npm-run-all --parallel test:watch open:src lint:watch",
    "start": "node bootstrap.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "heroku": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"test/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"test/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"test/**/*.spec.js\" && cat ./coverage/lcov.info | coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "body-parser": "^1.15.2",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "es6-promise": "^3.2.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "isomorphic-fetch": "^2.2.1",
    "isparta": "4.0.0",
    "kinetic": "^5.2.0",
    "localforage": "^1.4.2",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "npm-run-all": "1.8.0",
    "object-assign": "4.1.0",
    "react": "^15.2.1",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "redux": "3.5.2",
    "redux-form": "^5.2.4",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-persist": "^3.2.2",
    "redux-thunk": "^2.1.0",
    "rimraf": "2.5.2",
    "route-cache": "^0.2.4",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "sudoku": "0.0.2",
    "webpack": "5.42.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
